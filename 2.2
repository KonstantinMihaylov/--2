#include <stdio.h>
 
long int arr[100];
long int Fibbonachi(long int number)
{

  if (arr[number] == 0)
  {
    if (number == 1 || number == 2)
    {
      arr[number] = 1;
    } 
    else 
    {
      arr[number] = Fibbonachi(number - 1) + Fibbonachi(number - 2);
    }
  }

  return arr[number];
}
