#include <stdio.h>

long int arr[100];
long int get_fibbonachi_via_recursion(long int number)
{
  if (arr[number] == 0)
  {
    if (number == 1 || number == 2)
    {
      arr[number] = 1;
    } 
    else 
    {
      arr[number] = get_fibbonachi_via_recursion(number - 1) + get_fibbonachi_via_recursion(number - 2);
    }
  }

  return arr[number];
}
