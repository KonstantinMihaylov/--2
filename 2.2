#include <stdio.h>

long int get_fibbonachi_via_recursion(long int number)
{
  static long int array_of_elements_of_the_fibonacci_sequence[50];
  if (array_of_elements_of_the_fibonacci_sequence[number] == 0)
  {
    if (number == 1 || number == 2)
    {
      array_of_elements_of_the_fibonacci_sequence[number] = 1;
    } 
    else 
    {
      array_of_elements_of_the_fibonacci_sequence[number] = get_fibbonachi_via_recursion(number - 1) + get_fibbonachi_via_recursion(number - 2);
    }
  }

  return array_of_elements_of_the_fibonacci_sequence[number];
}
