#include <stdio.h>
 
long int Fibbonachi(long int number)
{

    if(number != 0 && number != 2 && number != 1)
    {
      return Fibbonachi(number - 1) + Fibbonachi(number - 2);
    }
    if (number == 0) 
    {
      return 0;
    }

    if (number == 1 || number == 2)
    {
      return 1;
    } 
  
}
