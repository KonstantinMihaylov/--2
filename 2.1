#include <stdio.h>

long get_fibbonachi_via_cycle(long int number)
{
  
  long int F1 = 1;
  long int F2 = 1;
  long int next_number;

  if (number <= 2)
  {
    return 1;
  }
  else 
  {
    for(int i = 3; i <= number; i++) 
    {
      next_number = F1 + F2; 
      F1 = F2; 
      F2 = next_number; 
    }
  }

  return F2;
}
